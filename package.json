{
  "name": "gatsby-cms-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "gatsby-CMS-boilerplate",
  "author": "Arthur Molinos",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "lint": "yarn lint:ts && yarn lint:css && yarn lint:prettier",
    "lint:ts": "eslint . --ext .js --ext .ts --ext .tsx && tsc --noEmit",
    "lint:css": "stylelint './src/**/*.{js,ts,tsx}'",
    "lint:prettier": "prettier --check './src/**/*.{js,ts,tsx}'",
    "fix": "yarn fix:ts && yarn fix:prettier",
    "fix:ts": "yarn lint:ts --fix",
    "fix:prettier": "yarn lint:prettier --write",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.2",
    "gatsby": "^3.9.0",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^1.9.0",
    "gatsby-plugin-manifest": "^3.9.0",
    "gatsby-plugin-react-helmet": "^4.9.0",
    "gatsby-plugin-robots-txt": "^1.6.9",
    "gatsby-plugin-sharp": "^3.9.0",
    "gatsby-plugin-sitemap": "^4.5.0",
    "gatsby-plugin-styled-components": "^4.9.0",
    "gatsby-transformer-sharp": "^3.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.2",
    "@types/styled-components": "^5.1.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "install-peerdeps": "^3.0.3",
    "prettier": "^2.3.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.3.5"
  }
}
